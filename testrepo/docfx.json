{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "crrForTest/crrsource/",
        "dest": "test",
        "exclude": [
          "README.md",
          "LICENSE.md",
          "**/includes/**"
        ]
      },
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/preview-test-testrepo/breadcrumb/toc.json",
      "extendBreadcrumb": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "preview-test-testrepo"
  }
}
